[Unit]
Description=Gunicorn application server running AirBnB_clone_v4
Author=Brennan D Baraban
After=network.target

[Service]
User=ubuntu
Group=www-data
RestartSec=5
WorkingDirectory=/home/ubuntu/AirBnB_clone_v4
ExecStart=/path/to/gunicorn --workers 3 --bind 0.0.0.0:5003 web_dynamic.2-hbnb:app >> /tmp/airbnb-access.log 2>> /tmp/airbnb-error.log
Restart=always

[Install]
WantedBy=multi-user.target
